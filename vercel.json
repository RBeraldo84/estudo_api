{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/hoteis/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/usuarios/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  